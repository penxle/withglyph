    "fast-json-stable-stringify": "^2.1.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-sse": "^2.5.3",
    "object-traversal": "^1.0.1",
    "radash": "^12.1.0",
    "recast": "^0.23.6",
    "svelte": "^4.2.15",
    "ts-pattern": "^5.1.1",
    "wonka": "^6.3.4"
  },
  "devDependencies": {
    "@withglyph/tsconfig": "workspace:^",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.4.0"
  }
